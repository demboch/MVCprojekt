@model IEnumerable<MVCprojekt.Models.BronViewModels>

@{
    ViewBag.Title = "Statystyki";
}
<h2>Statystyki dotyczące strony</h2>
<h3>Ilość broni w magazynie: @ViewBag.Bron</h3>
<table>
    <tr>
        <th class="text-center">
            <li style="list-style-type: none; margin-right: 5px">Nazwa sklepu</li>
        </th>
        <th class="text-center">
            <li style="list-style-type: none">Ilość broni</li>
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td class="text-center">
                @Html.DisplayFor(modelItem => item.Nazwa) Militaria
            </td>
            <td class="text-center">
                @item.Ilosc
            </td>
        </tr>
    }
</table>
<h3>Ilość użytkowników strony: @ViewBag.Users</h3>




